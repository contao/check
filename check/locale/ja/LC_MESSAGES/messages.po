msgstr "インストールを開始"

#: check/views/installer.phtml:52
msgid "Installation failed"
msgstr ""

#: check/views/installer.phtml:55
msgid "Installation complete"
msgstr "インストールを完了"

#: check/views/installer.phtml:56
